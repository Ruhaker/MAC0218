<h1>Creating supervisor</h1>
<%= link_to  "Voltar", "/" %>

<%= form_tag "/supervisor/create", :method => "post" do %>
    <%= text_field_tag :login_field, nil, placeholder: "email" %><br>
    <%= text_field_tag :full_name, nil, placeholder: "nome" %><br>
    <%= text_field_tag :cpf, nil, placeholder: "cpf" %><br>
    <%= password_field_tag :password, nil, placeholder: "senha" %><br>
    <input type="submit" value="Criar" />
<% end %>